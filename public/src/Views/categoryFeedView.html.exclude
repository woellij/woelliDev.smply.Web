<!doctype html>
<html>
<body>
   
    <div class="row" style="height:100%">
        <div id="grid" style="height:100%" data-bind="foreach: items">
            <a href="#" class="th item">
                <img class="pinImage" style="width:100%" data-bind="attr: { src: image }" />
                <div data-bind="click: $root.boardSelected">
                    <h4 class="list-group-item-heading" data-bind="text: name"></h4>
                </div>
            </a>
        </div>
    </div>
</body>
</html>